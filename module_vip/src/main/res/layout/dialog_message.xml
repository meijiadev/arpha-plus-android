<?xml version="1.0" encoding="utf-8"?>
<com.caldremch.widget.round.RoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="270dp"
    android:layout_height="136dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:rl_radius="14dp"
    app:rl_backgroundColor="@color/white">
    <TextView
        android:id="@+id/tvMesTitle"
        android:layout_width="238dp"
        android:layout_height="22dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:text="@string/message_dialog_title"
        android:textSize="16dp"
        android:textColor="@color/black_333333"
        android:gravity="center"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/gray_5C3C3C43"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="44dp"
        />
    <View
        android:layout_width="1px"
        android:layout_height="44dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        />

    <com.caldremch.widget.round.RoundTextView
        android:id="@+id/tvCancel"
        android:layout_width="134dp"
        android:layout_height="43dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:text="@string/message_no"
        android:gravity="center"
        android:textColor="@color/blue_0A84FF"
        android:textSize="16dp"
        app:rl_radius="@dimen/sp_14"
        app:rl_backgroundColor="@color/white"
        />

    <com.caldremch.widget.round.RoundTextView
        android:id="@+id/tvConfirm"
        android:layout_width="134dp"
        android:layout_height="43dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="@string/message_yes"
        android:gravity="center"
        android:textColor="@color/blue_0A84FF"
        android:textSize="16dp"
        app:rl_radius="@dimen/sp_14"
        app:rl_backgroundColor="@color/white"
        />



</com.caldremch.widget.round.RoundRelativeLayout>