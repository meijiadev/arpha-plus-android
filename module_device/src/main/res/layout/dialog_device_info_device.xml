<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shape_sheet_dialog_bg1">


    <TextView
        android:id="@+id/tv_info_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/device_info"
        android:textColor="@color/black_4F4C4B"
        android:textSize="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="5dp"
        android:background="@color/text_BC9E70"
        app:layout_constraintLeft_toLeftOf="@+id/tv_del_dialog_title"
        app:layout_constraintRight_toRightOf="@+id/tv_del_dialog_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_del_dialog_title" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_check_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:minWidth="250dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_info_dialog_title">

        <TextView
            android:id="@+id/tv_check_news1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_news_1"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_check_news2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/check_news_2"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_check_news1" />

        <TextView
            android:id="@+id/tv_check_news3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/check_news_3"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_check_news2" />

        <TextView
            android:id="@+id/tv_check_news4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/check_news_4"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_check_news3" />

        <TextView
            android:id="@+id/tv_check_news5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/check_news_5"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_check_news4" />

        <TextView
            android:id="@+id/tv_check_news6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/check_news_6"
            android:textColor="@color/red_E3243B"
            android:textSize="14dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_check_news5" />


        <TextView
            android:id="@+id/tv_check_news11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="title"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_check_news1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_check_news1" />

        <TextView
            android:id="@+id/tv_check_news22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="title"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_check_news2"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_check_news2" />

        <TextView
            android:id="@+id/tv_check_news33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="title"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_check_news3"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_check_news3" />

        <TextView
            android:id="@+id/tv_check_news44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="title"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_check_news4"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_check_news4" />

        <TextView
            android:id="@+id/tv_check_news55"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="title"
            android:textColor="@color/black_4F4C4B"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_check_news5"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_check_news5" />


    </androidx.constraintlayout.widget.ConstraintLayout>


    <com.caldremch.widget.round.RoundButton
        android:id="@+id/btn_del_device_go2"
        android:layout_width="148dp"
        android:layout_height="36dp"
        android:layout_marginTop="44dp"
        android:gravity="center"
        android:text="@string/next"
        android:textColor="@color/white"
        android:textSize="14dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_check_info"
        app:rl_backgroundColor="@color/grey_4F4C4B"
        app:rl_radius="30dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_del_device_go2" />
</androidx.constraintlayout.widget.ConstraintLayout>